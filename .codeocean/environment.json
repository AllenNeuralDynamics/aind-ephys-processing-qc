					"version": "0.2.3"
				},
				{
					"name": "aind-qcportal-schema",
					"version": "0.6.4"
				},
				{
					"name": "s3fs",
					"version": "2025.5.1"
				},
				{
					"name": "spikeinterface[full,widgets]",
					"version": "0.103.0"
