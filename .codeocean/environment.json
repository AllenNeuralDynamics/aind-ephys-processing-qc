{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip": {
			"packages": [
				{
					"name": "aind-data-schema",
					"version": "1.3.0"
				},
				{
					"name": "aind-log-utils",
					"version": "0.2.3"
				},
				{
					"name": "aind-qcportal-schema",
					"version": "0.4.0"
				},
				{
					"name": "spikeinterface[full,widgets]",
					"version": "0.102.1"
				},
				{
					"name": "wavpack-numcodecs",
					"version": "0.2.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}